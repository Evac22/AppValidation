<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <appSettings>
    <add key="FolderPath" value="C:\Users\Vlad\OneDrive\Рабочий стол\validation\A"/>
    <add key="ResultFolderPath" value="C:\Users\Vlad\OneDrive\Рабочий стол\validation\B" />
    <add key="SubfolderName" value="C:\Users\Vlad\OneDrive\Рабочий стол\validation\B\C" />
  </appSettings>

  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
